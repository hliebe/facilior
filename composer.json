{
  "name": "lotzer/facilior",
  "description": "A cli tool to interact easy with multiple databases.",
  "homepage": "http://www.noahlotzer.com",
  "license": "MIT",
  "type": "library",
  "keywords": ["deployment", "database", "facilior"],
  "require": {
    "php": ">=5.3",
    "symfony/console": "~2.2",
    "symfony/yaml": "~3.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.1.7",
    "satooshi/php-coveralls": "~1.0.1",
    "php-mock/php-mock": "~1.0.1"
  },
  "autoload": {
    "psr-4": {
      "Lotzer\\Facilior\\": "./src/Facilior"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lotzer\\Facilior\\Tests\\": "./tests/Neusta/Facilior"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "bin": [
    "bin/facilior"
  ]
}